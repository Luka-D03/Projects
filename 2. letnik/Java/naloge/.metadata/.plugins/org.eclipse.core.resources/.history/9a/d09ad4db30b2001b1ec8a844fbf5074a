import java.util.Scanner;

public class naloga3 {
//Napiši program, ki prebere 10 števil in izpiše najveèje in najmanjše število. 
//Na intervalu med drugim najveèjim vnesenim številom in najveèjim vnesenim številom sešteje vsako drugo število 
//in izpiše rezultat.

	public static void main(String[] args) {
		try(Scanner vhod = new Scanner(System.in)){
		
			int max = -999999999;
			int min = 999999999;
			int max2 = -999999999;
			int vsota = 0;
			
			for(int i = 1; i <= 10; i++) {	
				System.out.print("Vnesi "+i+". število: ");
				int x = vhod.nextInt();
				
				if(x > max) {
					max = x;
				} else {
					max2 = x;
				}
			}
			System.out.print("Vnesi "+max+". število: ");
			System.out.print("Vnesi "+max2+". število: ");
		}
	}
}
